{
  "manifest_version": 3,
  "name": "Car",
  "version": "1.0",
  "description": "Drive a car around your webpages!",
  "permissions": ["activeTab", "scripting", "storage"],
  "action": {
    "default_icon": "car_icon.png",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    {
      "resources": ["images/car1.png", "images/car2.png", "images/car3.png"],
      "matches": ["<all_urls>"]
    }
  ]
}








